:set nocompatible
:set background=dark
:syntax on
:set number
:set bs=2
:set nobackup
:set nowritebackup
:set noswapfile
:set showmatch
:set showcmd
:set ruler
:set title
:set mouse=a
":set spell
:set history=1000
:set incsearch
:set hlsearch
:set wildmenu
:set tabstop=4
:set autoindent
:set ignorecase

:command W w
:command Wq wq

:map <C-T> :tabnew<CR>

:set matchpairs+=<:>



